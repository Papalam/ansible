				reuseSessions="dontuse"
				sessionMaxAge="20"
				poolSize="10"
				poolTimeout="5"/>
		<point name="MobileAccounting"
				alias="MobileAccounting.1cws"
				enable="true"
				reuseSessions="dontuse"
				sessionMaxAge="20"
				poolSize="10"
				poolTimeout="5"/>
		<point name="MobileEntrepreneur"
				alias="MobileAcc.1cws"
				enable="true"
				reuseSessions="dontuse"
				sessionMaxAge="20"
